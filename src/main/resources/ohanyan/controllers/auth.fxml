        <Label layoutX="164.0" layoutY="91.0" text="Пароль:"/>
        <TextField fx:id="fldLogin" layoutX="106.0" layoutY="51.0" prefHeight="25.0" prefWidth="170.0"
                   AnchorPane.leftAnchor="106.0" AnchorPane.rightAnchor="106.0"/>
        <PasswordField fx:id="fldPass" layoutX="106.0" layoutY="116.0" prefHeight="25.0" prefWidth="200.0"
                       AnchorPane.leftAnchor="106.0" AnchorPane.rightAnchor="106.0"/>
    </children>
